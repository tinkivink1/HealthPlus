@model HealthPlus.Models.Users
<
		<div class="form_auth_block">
			    <form asp-action="LoginL">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
			  <div class="form-group">
                <label asp-for="client_email" class="control-label"></label>
                <input asp-for="client_email" class="form-control" />
                <span asp-validation-for="client_email" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="client_password" class="control-label"></label>
                <input asp-for="client_password" class="form-control" />
                <span asp-validation-for="client_password" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="LoginL" class="btn btn-primary"/>
            </div>
			</form>
		  </div>

		@section Scripts {
		@{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
	}

